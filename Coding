package Test3;
import java.util.Arrays;
import java.util.Scanner;
public class Quiz3 {

	public static void main(String[] args, int i, int[] a) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner (System.in);
		int [] num = new int [5];
		int choice;
	
		System.out.println("Please enter the element of the array ");
		for(int i1=0; i1<num.length; i1++)
            num [i1] =sc.nextInt();
		
		for(int j=0; j<num.length; j++)
			System.out.println(num[i]);
		
		
		System.out.println("Please enter an integer");
		choice = sc.nextInt();
       	 
		Find(num, choice);
		Arrays.sort(num); 
		
		System.out.println("Elements of array sorted in ascending order: ");  
		for (int y = 0; y < num.length; y++)   
		{       
		System.out.println(num[y]);   
		}   
	}

	
	public static void Print (int k, int l) {

	     System.out.println("Integer " + k + ": " + l);
	}
	
	
		public static void Find(int[ ] no, int searchValue) {
			  for (int j=0; j < no.length; j++) {
				 if( j == searchValue) {
					  System.out.println("The search value is " + no[j]);
					  break;
				 }
			  }		
		}	
	
		
	}
